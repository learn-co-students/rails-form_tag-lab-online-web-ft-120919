<h3>Student Form</h3>
<!--<%=@student.first_name%>
<%=@student.last_name%>-->

    <%= form_tag students_path do %>

      <label for="student[first_name]">First Name:</label><br> 
      <%= text_field_tag :'student[first_name]', @student.first_name %><br><br>

      <label for="student[last_name]">Last Name:</label><br> 
      <%= text_field_tag :'student[last_name]', @student.last_name %><br><br>

      <%= submit_tag "Submit Student" %>
    <% end %>

<%= session[:form_params] if session[:form_params] %>